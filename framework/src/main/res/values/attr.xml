        <attr name="bordersWidth" format="reference|dimension" />
        <attr name="checklineColor" format="reference|color" />
        <attr name="checklineWidth" format="reference|dimension" />
        <attr name="uncheckcircleColor" format="reference|color" />
        <attr name="uncheckcircleWidth" format="reference|dimension" />
